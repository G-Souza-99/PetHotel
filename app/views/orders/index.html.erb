        <div class="co-12 colsm-6 col-md-4 mb-3">
          <div class="card-product">
            <%= cl_image_tag order.place.photo.key %>
            <div class="card-product-infos">
              <h2><%= order.place.name.capitalize %></h2>
              <p><%= order.checkin %> to <%= order.checkout %></p>
              <p><strong>Total 50â‚¬</strong></p>
            </div>
          </div>
        </div>
    <% end %>
  </div>
</div>
