<head>
  <link rel="scss" href="../../../assets/stylesheets/pages/new.scss" />
</head>

<div class="container">
  <h2>Log in</h2>

  <%= simple_form_for(resource, as: resource_name, url:
  session_path(resource_name)) do |f| %>
  <div class="form-inputs">
      <%= f.input :email, required: false, autofocus: true, input_html: {
      autocomplete: "email", :value => 'user@domain.com' } %>
    </div>
    <div>
      <%= f.input :password, required: false, input_html: { autocomplete:
      "current-password", :value => '**********'} %>
    </div>
    <div>
      <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
    </div>
  </div>

  <div class="form-actions">
    <button><%= f.button :submit, "Log in" %></button>
  </div>
  <% end %>

  <div class="devise-shared-links">
    <%= render "devise/shared/links" %>
  </div>
</div>
